using System;
class Program{
      static void Main()
    {
        Console.Write($"Zadej cislo, ale pouzivej 0 a 1: ");
        string word = Console.ReadLine();

        for (int i = 0; i < word.Length; i++)
        {
            if (word[i] != 1 || word[i] != 0)
            {
                Console.WriteLine($"Cislo obsuhuje cisla mimo 0 a 1");
                break;
            }
        }
        Console.WriteLine($"Cislo neobsuhuje zadna cisla krome 0 a 1");

    }
}