#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
	srand(time(NULL));
	int rcis = rand() % 5;
	int arr[5];
	printf("Zadej 5 libovolych pismenek:");

	for (int i = 0; i < 5; i++) {
		scanf_s(" %c", &arr[i]);

	}
	printf("Rand pismenko z tohoto pole: %c\n", arr[rcis]);
return 0;
}