#include <stdio.h>
#include <string.h>

int main() {
	char str1[99];
	char str2[99];
	printf("Zadej 1. slovo:\n");
	scanf("%s", str1);
	printf("Zadej 2. slovo:\n");
	scanf("%s", str2);
	int len1 = strlen(str1);
	int len2 = strlen(str2);

	if (len1 != len2) {
		printf("Slova nejsou stejna.\n");
		return 0;
	}

	for (int i = 0; i < len1; i++) {
		if (str1[i] != str2[i]) {
			printf("Slova nejsou stejna.\n");
			return 0;
		}
	}

	printf("Slova jsou stejna :P\n");

	return 0;
}